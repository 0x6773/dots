#! /bin/zsh

source ~/.dot/aliasrc

source ~/.dot/zshoptrc

source ~/.dot/colorrc

source ~/.dot/gccrc

source ~/.dot/aliasrc

source ~/.dot/netrc

source ~/.dot/volumerc

# source ~/.dot/javarc

unset SSH_ASKPASS

is_mac && eval $(gdircolors -b ~/.dot/LS_COLORS)

swap()         
{
  local TMPFILE=tmp.$$;
	mv "${1}" $TMPFILE && mv "${2}" "${1}" && mv "${TMPFILE}" "${2}";
}

if is_mac; then
  alias l="gls -lahG --color"
  alias ls="gls -G --color"
  alias ll="gls -lhG --color"
fi


